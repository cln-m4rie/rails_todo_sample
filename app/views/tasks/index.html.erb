<table>
  <thead>
  <tr>
    <th>State</th>
    <th>Limit</th>
    <th>Task</th>
    <th></th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  <% @tasks.each do |t| %>
    <tr>
      <td><%= t.status %></td>
      <td><%= t.due_date %></td>
      <td><%= t.name %></td>
      <td>
        <%= link_to t.name, task_path(id: t.id) %>
      </td>
      <td>
        <button type="submit">delete</button>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>